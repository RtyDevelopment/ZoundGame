ds_list_add(_y, irandom(room_height));
ds_list_add(_l, irandom_range(50, 100));
ds_list_add(_t, 1);
n = irandom_range(5, 40);

keyboard_string = "";
ipstr = ""

if (view_enabled==true) {
    width = view_wview[0];
    height = view_hview[0];
} else {
    width = room_width;
    height = room_height;
}

if (os_get_config()=="OEM") {
    menu = 1;
} else {
    menu = 0;
}

name = "Steve";
port = 43520;
//key = "2349872398479832749873249862354678687234987235896734985678934659872305434598743895789374569734658726983657298698619835031758990135098237580873752908235780613809"
key = "";
for (var i=0; i&lt;160; i++) key += string(irandom(9));

selfip = "?";
selfhash = string(random(100000)*random(100000))+string(random(100000));

client_key = ds_list_create();
client_ip = ds_list_create();
client_name = ds_list_create();
client_req = ds_list_create();
client_del = ds_list_create();

//NetworkStuff
//IP detection server
selfserver = -1;
selfport = round(random_range(7, 60)*1000);
selfbuffer = buffer_create(128, buffer_fixed, 1);
while (selfserver&lt;0) {
    selfport++;
    selfserver = network_create_server(network_socket_udp, selfport, 32);
}

//LAN detection:
//listening device
listendevm = true;
listenserver = network_create_server(network_socket_udp, 6510, 32);
if (listenserver&lt;0) {
    listendevm = false;
    listenserver = network_create_socket(network_socket_udp);
    var package = buffer_create(200, buffer_fixed, 1);
    buffer_seek(package, buffer_seek_start, 0);
    buffer_write(package, buffer_string, key);
    buffer_write(package, buffer_string, string_copy(name, 1, 40));
    network_send_udp(listenserver, "127.0.0.1", 6510, package, buffer_tell(package));
}

//broadcasting server
bcbuffer = buffer_create(200, buffer_fixed, 1);
bcserver = -1;
bcserverport = 6510;
while (bcserver&lt;0) {
    bcserverport++;
    bcserver = network_create_server(network_socket_udp, bcserverport, 32);
}

// Setup a timer so we can broadcast the server IP out to local clients looking...
alarm[0]=5;
