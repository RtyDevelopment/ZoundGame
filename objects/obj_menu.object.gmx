        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// UDP server code - Detect servers "broadcast"
var recvip = ds_map_find_value(async_load, "ip");
if (recvip=="127.0.0.1" || recvip==selfip) {
    if (listendevm==true) {
        //broadcast another client on device to network
    }
} else {
    if (selfip!="?") {
        if (ds_map_find_value(async_load, "id")==listenserver) {
            
            var recvkey = buffer_read(ds_map_find_value(async_load, "buffer"), buffer_string);
            var recvname = buffer_read(ds_map_find_value(async_load, "buffer"), buffer_string);
                
            // Add to our list...if we don't already have it!
            if (ds_list_find_index(client_key, recvkey)&lt;0 &amp;&amp; key!=recvkey) {
                ds_list_add(client_key, recvkey);
                ds_list_add(client_ip, recvip);
                ds_list_add(client_name, recvname);
                ds_list_add(client_req, 0);
                ds_list_add(client_del, 0);
                show_debug_message("New server found at: "+recvip+"   called: "+recvname);
            }    
        } 
    } else {
        if (ds_map_find_value(async_load, "id")==selfserver) {
            if (buffer_read(ds_map_find_value(async_load, "buffer"), buffer_string)==md5_string_unicode(selfhash)) {
                selfip = ds_map_find_value(async_load, "ip");
                network_destroy(selfserver);
                buffer_delete(selfbuffer);
            }
        }
    }
}
