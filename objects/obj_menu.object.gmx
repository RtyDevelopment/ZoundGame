buffer_write(bcbuffer, buffer_string, key);
buffer_write(bcbuffer, buffer_string, string_copy(name, 1, 40));
network_send_broadcast(bcserver, 6510, bcbuffer, buffer_tell(bcbuffer));

if (selfip=="?") {
