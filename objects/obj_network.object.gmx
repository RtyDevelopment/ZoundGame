var file;
file = file_text_open_append(working_directory+"\export.txt");
file_text_writeln(file);
file_text_write_string(file, " # PEERS # ");
for (var i=0; i&lt;ds_list_size(net_peer_id); i++) {
    file_text_write_string(file, " - "+string(i)+": - ");
    file_text_writeln(file);
    file_text_write_string(file, "ID       : ");
    file_text_write_string(file, string(ds_list_find_value(net_peer_id, i)));
    file_text_writeln(file);
    file_text_write_string(file, "Key      : ");
    file_text_write_string(file, sha1_string_unicode(ds_list_find_value(net_peer_key, i)));
    file_text_writeln(file);
    file_text_write_string(file, "IP       : ");
    file_text_write_string(file, ds_list_find_value(net_peer_ip, i));
    file_text_writeln(file);
    file_text_write_string(file, "Port     : ");
    file_text_write_string(file, string(ds_list_find_value(net_peer_port, i)));
    file_text_writeln(file);
    file_text_write_string(file, "NetType  : ");
    switch (ds_list_find_value(net_peer_nettype, i)) {
        case NET_UDP:
            file_text_write_string(file, "UDP"); break;
        case NET_TCP:
            file_text_write_string(file, "TCP_GM"); break;
        case NET_TCPRAW:
            file_text_write_string(file, "TCP"); break;
        case NET_BROADCAST:
            file_text_write_string(file, "UDP_BC"); break;
        case NET_HTTP:
            file_text_write_string(file, "HTTP"); break;
    }
    file_text_writeln(file);
    file_text_write_string(file, "Name     : ");
    file_text_write_string(file, ds_list_find_value(net_peer_name, i));
    file_text_writeln(file);
    file_text_write_string(file, "Ping     : ");
    file_text_write_string(file, string(ds_list_find_value(net_peer_ping, i)));
    file_text_writeln(file);
    file_text_write_string(file, "Last ping: ");
    file_text_write_string(file, string(ds_list_find_value(net_peer_lastping, i)));
    file_text_writeln(file);
    file_text_write_string(file, "Last pong: ");
    file_text_write_string(file, string(ds_list_find_value(net_peer_pingrecv, i)));
    file_text_writeln(file);
    file_text_write_string(file, "Save type: ");
