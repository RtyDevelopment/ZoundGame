            <string>if (global.networkmenu==true) {
    if (keyboard_string!="") {
        ipstr += keyboard_string;
        keyboard_string = "";
    }
    if (keyboard_check_pressed(vk_backspace)==true) ipstr = string_delete(ipstr, string_length(ipstr), 1);
